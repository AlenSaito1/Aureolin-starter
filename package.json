  "repository": {
    "url": "https://github.com/Alensaito1/aureolin"
  },
  "issues": "https://github.com/Alensaito1/aureolin",
  "devDependencies": {
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "aureolin": "github:alensaito1/aureolin",
