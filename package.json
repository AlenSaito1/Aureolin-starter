{
  "name": "aureolin",
  "version": "0.1.0",
  "description": "Rest API Framework",
  "main": "dist",
  "types": "dist",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "dev": "NODE_ENV=development nodemon --ext \"ts,tsx\" src/main.ts",
    "start": "NODE_ENV=production node dist/main.js",
    "prebuild": "rimraf dist",
    "fmt": "prettier --config .prettierrc \"src/**/*.{ts,tsx}\" --write"
  },
  "keywords": [
    "api",
    "rest",
    "koa",
    "framework",
    "example",
    "jsx",
    "typescript"
  ],
  "author": {
    "name": "Alen Yohannan",
